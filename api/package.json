    "esm": "^3.2.25",
    "express": "^4.17.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "standard": "^16.0.3",
    "supertest": "^6.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
